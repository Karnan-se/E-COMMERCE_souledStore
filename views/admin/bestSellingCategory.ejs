                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th>Rank</th>
                            <th>Product Name</th>
                            <th>Total Quantity Sold</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% categories.forEach((category, index) => { %>
                            <tr>
                                <td><%= index + 1 %></td>
                                <td><%= category.categoryname %></td>
                                <td><%= category.totalQuantity %></td>
                            </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div>
        </div>

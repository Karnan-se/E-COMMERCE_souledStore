                        <script>
                           async function SearchProduct(){
                              const search=  document.getElementById("searchProduct")
                             const keywords= search.value
                             const response = await fetch(`/searchProduct?keyword=${keywords}`,{
                                method:"GET",
                             })
                             if(!response){
                                throw new Error("Internal Server Error ")
                             }
                             const data= await response.json()
                             console.log(data.product);
                             if(data){
                                updateProductList(data.product);

                             }
                            }

function updateProductList(products) {
    const container = document.getElementById("product-container");
    container.innerHTML = ''; // Clear existing products
    
    const oneDay = 24 * 60 * 60 * 1000;
    const now = new Date();
    
    products.forEach(product => {
        const createdat = new Date(product.createdat);
        const timediff = now - createdat;
        const isNew = timediff < oneDay;
        
        const productHTML = `
            <div class="col-lg-4 col-md-4 col-12 col-sm-6">
                <div class="product-cart-wrap mb-30">
                    <div class="product-img-action-wrap productofindex">
                        <div class="product-img product-img-zoom">
                            <a href="/shop-product-right?productId=${product._id}">
                                <img class="default-img" src="/temp/${product.images[0]}" alt="">
                                <img class="hover-img flying-Element" src="/temp/${product.images[1]}" alt="">
                            </a>
                        </div>
                        <div class="product-action-1">
                            <a aria-label="Add To Wishlist" class="action-btn hover-up wishlist" data-productId="${product._id}"><i class="fi-rs-heart"></i></a>
                            <a aria-label="Compare" class="action-btn hover-up" href="/shop-compare"><i class="fi-rs-shuffle"></i></a>
                        </div>
                        <div class="product-badges product-badges-position product-badges-mrg">
                            ${isNew ? '<span class="new">New</span>' : ''}
                        </div>
                    </div>
                    <div class="product-content-wrap">
                        <div class="product-category">
                            <a href="/shop-grid-right">${product.productname}</a>
                        </div>
                        <h2><a href="/shop-product-right">${product.productname}</a></h2>
                        <div class="rating-result" title="90%">
                            <span>
                                <span>90%</span>
                            </span>
                        </div>
                        <div class="product-price">
                            <span>&#x20b9 ${product.price}</span>
                            <span class="${product.orginalPrice > 0 ? 'old-price' : 'd-none'}">
                                ${product.orginalPrice ? product.orginalPrice : ''}
                            </span>
                        </div>
                        <div class="product-action-1 show">
                            <a aria-label="Add To Cart" class="action-btn hover-up fi-rs-shopping-bag-add addtoCart" data-index="${product._id}"></a>
                        </div>
                    </div>
                </div>
            </div>
        `;
        
        container.insertAdjacentHTML('beforeend', productHTML);
    });
}

                            

                                     <div class="product-img-action-wrap">
                                         <div class="product-img product-img-zoom">
                                             <a href="/shop-product-right?productId=<%= product[i]._id %>">
                                                 <a href="/shop-product-right?productId=<%= product[i]._id %>">
                          
                                                 <img class="default-img" src="/temp/<%= product[i]?.images[0] %>" alt="">
                                                 <img class="hover-img" src="/temp/<%= product[i]?.images[1] %>" alt="">
                                             </a>
                                         </div>

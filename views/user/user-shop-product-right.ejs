                                        <script>
                                           async  function getStock(btnelement) {
                                                const size = btnelement.getAttribute("data-size");
                                                const productId = btnelement.getAttribute("data-product");
                                                console.log(productId)
                                                console.log(`size selected ${size}`);
                                                const response  = await fetch(`/checkStock?stockSize=${size}&productId=${productId}`,{
                                                    method:"GET",
                                                })
                                                if(!response){
                                                    throw new Error("Internal Server Error")
                                                }
                                                const data = await response.json();
                                                const stockValue = data.data;
                                               const stock = document.getElementById("stock")
                                               stock.innerHTML=`Available : ${stockValue}`
                                               stock.classList.add("text-success");
                                               if(stockValue<6){
                                                stock.classList.remove("text-success");
                                                stock.classList.remove("text-danger");
                                                stock.classList.add("text-warning");
                                               }
                                               if(stockValue<1){
                                                stock.innerHTML=`OUT OF STOCK: ${stockValue}`
                                               stock.classList.add("text-danger");
                                               const addtocart = document.getElementById("addtocart")
                                               addtocart.disabled =true
                                               addtocart.classList.add("btn-warning")
                                               addtocart.textContent= "OUT OF STOCK"
                                               }else{
                                                addtocart.classList.remove("btn-warning")
                                                addtocart.textContent= "Add to cart"
                                                addtocart.disabled = false




                                               }
                                            }
                                        </script>
                                        

        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            <table class="table shopping-summery text-center">
                                <thead>
                                    <tr class="main-heading">
                                        <th scope="col" colspan="2">Product</th>
                                        <th scope="col">Price</th>
                                        
                                        <th scope="col">Action</th>
                                        <th scope="col">Remove</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if (typeof wishList !== "undefined" && wishList.product.length > 0) { %>
                                    <% wishList.product.forEach((item)=>{  %>
                                    <tr>
                                        <td class="image product-thumbnail"><img src="/temp/<%=item.productId.images[0]%>" alt="#" style="width: 100px; height: auto;"></td>
                                        <td class="product-des product-name">
                                            <h5 class="product-name"><a href="/shop-product-right?productId=<%= item.productId._id%>"><%= item.productId.productname %></a></h5>
                                            <p class="font-xs">color : <%= item.productId.color %><br> materials : <%= item.productId.description%>
                                            </p>
                                        </td> 
                                        <td class="price" data-title="Price"><span><%= item.productId.price %></span></td>
                                        
                                        <td class="text-right" data-title="Cart">
                                            <button class="btn btn-sm" onclick="shopProductRight(this)" data-productId="<%= item.productId._id %>"><i class="fi-rs-shopping-bag mr-5"></i>Add to cart</button>
                                        </td>
                                        <td class="action" data-title="Remove"><a href="#"><i class="fi-rs-trash"></i></a></td>
                                    </tr>
                                    <% })%>

                                    <script defer>
                                       async function shopProductRight(btn){
                                            const productId = btn.getAttribute("data-productId");
                                            console.log(productId);
                                            window.location.href=`/shop-product-right?productId=${productId}`

                                        }
                                        </script>
                                        <%}else{%>
                                            
                                            <%}%>

                                    
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </section>
